{
  "name": "browsergameqa",
  "version": "1.0.0",
  "description": "Automated QA testing for browser-based games",
  "module": "src/api/index.ts",
  "main": "src/api/index.ts",
  "type": "module",
  "private": true,
  "bin": {
    "browsergame-qa": "src/cli/index.ts"
  },
  "exports": {
    ".": {
      "import": "./src/api/index.ts",
      "require": "./src/api/index.ts"
    }
  },
  "scripts": {
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "lint": "bun biome check .",
    "lint:fix": "bun biome check --write .",
    "format": "bun biome format --write .",
    "type-check": "tsc --noEmit",
    "cli": "npx tsx -r dotenv/config src/cli/index.ts",
    "demo": "bun demo-test.ts",
    "demo:throttled": "npx tsx -r dotenv/config throttled-test.ts",
    "demo:cleanup": "bun cleanup-sessions.ts",
    "quick-test": "npx tsx -r dotenv/config quick-test.ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "2.0.59",
    "@aws-sdk/client-s3": "^3.725.0",
    "@browserbasehq/sdk": "2.6.0",
    "@browserbasehq/stagehand": "2.5.0",
    "ai": "5.0.86",
    "commander": "14.0.2",
    "pino": "10.1.0",
    "pixelmatch": "^7.1.0",
    "sharp": "^0.34.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.2",
    "@types/aws-lambda": "^8.10.157",
    "@types/bun": "latest",
    "@vitest/coverage-v8": "^4.0.6",
    "pino-pretty": "^13.0.0",
    "vitest": "4.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
